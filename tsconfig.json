{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "jsx": "react-jsx",
    "noLib": false,
    "isolatedModules": true,
    "lib": ["ESNext"],
    "target": "ESNext",
    "outDir": "./dist",
  },
  "include": ["./*.ts"],
}
